/* CTF 1.8 */
trace {
	major = 1;
	minor = 8;
	byte_order = le;
	uuid = "ebda51e8-2451-11e7-b10d-34f39aeaad30";
	packet.header := struct {
		integer {
			size = 32;
			align = 32;
			signed = false;
			byte_order = le;
			base = 10;
			encoding = none;
		} magic;
		integer {
			size = 8;
			align = 8;
			signed = false;
			byte_order = le;
			base = 10;
			encoding = none;
		} uuid[16];
		integer {
			size = 16;
			align = 16;
			signed = false;
			byte_order = le;
			base = 10;
			encoding = none;
		} stream_id;
	} align(1);
};

