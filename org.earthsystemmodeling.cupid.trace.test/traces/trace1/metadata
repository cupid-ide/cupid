	tracer_minor = 2;
	esmf_trace_version = "0.1";
};

clock {
	name = sys_clock;
	description = "System clock";
	freq = 1000000000;
	offset_s = 0;
	offset = 0;
	precision = 0;
	absolute = false;
};

/* default */
stream {
	id = 0;
	packet.context := struct {
		integer {
			size = 64;
			align = 64;
			signed = false;
			byte_order = le;
			base = 10;
			encoding = none;
			map = clock.sys_clock.value;
		} timestamp_begin;
		integer {
			size = 64;
			align = 64;
			signed = false;
			byte_order = le;
			base = 10;
			encoding = none;
			map = clock.sys_clock.value;
