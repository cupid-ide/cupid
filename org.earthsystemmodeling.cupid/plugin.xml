            class="org.earthsystemmodeling.cupid.views.NUOPCView"
            icon="icons/sample.gif"
            id="org.earthsystemmodeling.cupid.views.NUOPCView"
            name="NUOPC View">
      </view>
      <category
            id="org.earthsystemmodeling.codequery"
            name="Code Query">
      </category>
      <view
